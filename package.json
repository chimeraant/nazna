{
  "dependencies": {
    "@morphic-ts/batteries": "^3.0.0",
    "@types/js-yaml": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-fp-ts": "^0.3.2",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fast-equals": "^2.0.4",
    "fp-ts": "^2.13.1",
    "fp-ts-std": "^0.15.0",
    "io-ts": "^2.2.19",
    "io-ts-types": "^0.5.19",
    "js-yaml": "^4.1.0",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "prettier": "^2.7.1",
    "simple-git": "^3.14.1",
    "ts-pattern": "^4.0.5"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.8",
    "@types/node": "^18.11.2",
    "eslint": "^8.25.0",
    "pnpm": "^7.13.4",
    "typescript": "^4.8.4",
    "vitest": "^0.24.1"
  },
  "scripts": {
    "build": "pnpm build:types && pnpm build:es6 && pnpm build:cjs && pnpm build:cli",
    "build:cjs": "swc src --out-dir dist/cjs --source-maps --config module.type=commonjs",
    "build:cli": "printf '#!/usr/bin/env node\nrequire(\"./cjs/index\").cli({process})();' > ./dist/cli.js && chmod +x ./dist/cli.js",
    "build:es6": "swc src --out-dir dist/es6 --source-maps",
    "build:types": "tsc --project tsconfig.dist.json",
    "test:fix": "pnpm build && ./dist/cli.js fix && git checkout -- package.json .github/workflows/release.yaml",
    "fix": "eslint --max-warnings=0 --ext .ts . --fix",
    "lint": "eslint --max-warnings=0 --ext .ts .",
    "pre-push": "pnpm pre-push:dirty && pnpm publish --dry-run",
    "pre-push:dirty": "CI=true pnpm install && pnpm build && pnpm lint",
    "test": "vitest"
  },
  "name": "nazna",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es6/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/es6/index.js"
  },
  "files": [
    "dist"
  ],
  "bin": "./dist/cli.js",
  "repository": "https://github.com/chimeraant/nazna"
}
