{
  "devDependencies": {
    "@types/node": "^18.8.5",
    "@vitest/coverage-c8": "^0.24.1",
    "eslint": "^8.25.0",
    "eslint-config-chimeraant": "^1.2.4",
    "fp-ts": "^2.12.3",
    "husky": "^8.0.1",
    "nazna-tsconfig": "^1.0.0",
    "pnpm": "^7.13.4",
    "prettier": "^2.7.1",
    "ts-pattern": "^4.0.5",
    "typescript": "^4.8.4",
    "typescript-language-server": "^2.0.1",
    "vite": "^3.1.7",
    "vitest": "^0.24.1"
  },
  "peerDependencies": {
    "vite": "^3.1.7"
  },
  "scripts": {
    "build:lib": "vite build",
    "build:type": "tsc --project tsconfig.lib.json --emitDeclarationOnly",
    "build": "pnpm build:lib && pnpm build:type",
    "fix": "eslint --max-warnings=0 --ext .ts . --fix",
    "lint": "eslint --max-warnings=0 --ext .ts .",
    "test": "vitest"
  },
  "name": "nazna",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "repository": "https://github.com/chimeraant/nazna.git",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "eslintConfig": {
    "extends": "chimeraant"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
