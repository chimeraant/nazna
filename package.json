{
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.8",
    "@types/node": "^18.8.5",
    "eslint": "^8.25.0",
    "eslint-config-chimeraant": "^1.2.4",
    "fp-ts": "^2.12.3",
    "husky": "^8.0.1",
    "nazna-tsconfig": "^1.0.0",
    "pnpm": "^7.13.4",
    "prettier": "^2.7.1",
    "ts-pattern": "^4.0.5",
    "typescript": "^4.8.4",
    "typescript-language-server": "^2.0.1",
    "vitest": "^0.24.1"
  },
  "scripts": {
    "build:es6": "swc src --out-dir dist/es6 --source-maps",
    "build:cjs": "swc src --out-dir dist/cjs --source-maps --config module.type=commonjs",
    "build:types": "tsc src/**.ts --outDir dist/types --skipLibCheck --declaration --declarationMap --emitDeclarationOnly",
    "build:cli": "printf '#!/usr/bin/env node\nrequire(\"./cjs/index\").cli();' > ./dist/nazna && chmod +x ./dist/nazna",
    "build": "pnpm build:types && pnpm build:es6 && pnpm build:cjs && nazna build cli",
    "fix": "eslint --max-warnings=0 --ext .ts . --fix",
    "lint": "eslint --max-warnings=0 --ext .ts .",
    "test": "vitest"
  },
  "name": "nazna",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "repository": "https://github.com/chimeraant/nazna.git",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es6/index.mjs",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/es6/index.mjs"
  },
  "files": [
    "dist"
  ],
  "eslintConfig": {
    "extends": "chimeraant"
  },
  "bin": "./dist/cli.js"
}
